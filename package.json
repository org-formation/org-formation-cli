    "@aws-sdk/lib-storage": "^3.637.0",
    "@aws-sdk/middleware-sdk-s3": "^3.635.0",
    "@aws-sdk/middleware-signing": "^3.620.0",
    "@smithy/property-provider": "^3.1.3",
    "@smithy/types": "^3.3.0",
    "archiver": "^5.3.1",
    "commander": "^2.20.0",
